[Unit]
Description=Uvicorn instance to serve myproject
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/home/ubuntu2/shinstall
Environment="PATH=/home/ubuntu2/shinstall/python_test/bin"
ExecStart=/home/ubuntu2/shinstall/python_test/bin/gunicorn --workers 1 -k uvicorn.workers.UvicornWorker main:app --bind 0.0.0.0:8080

[Install]
WantedBy=data.service